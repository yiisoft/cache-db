{
    "name": "yiisoft/cache-db",
    "type": "library",
    "description": "Yii Caching Library - DB Handler",
    "keywords": [
        "yii",
        "framework",
        "cache",
        "psr-16",
        "db"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/cache-db/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "chat": "https://t.me/yii3en",
        "source": "https://github.com/yiisoft/cache-db"
    },
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "psr/log": "^2.0|^3.0",
        "psr/simple-cache": "^2.0|^3.0",
        "yiisoft/db": "^1.0"
    },
    "suggest": {
        "yiisoft/log": "^2.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.2",
        "phpunit/phpunit": "^9.6|^10.1",
        "rector/rector": "^0.18.3",
        "roave/infection-static-analysis-plugin": "^1.25|^1.30",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.8|^5.8",
        "yiisoft/cache": "^3.0",
        "yiisoft/log": "^2.0"
    },
    "provide": {
        "psr/simple-cache-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Cache\\Db\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Cache\\Db\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "yiisoft/config": true
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
